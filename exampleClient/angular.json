{
  "projects": {
    "exampleClient": {
      "sourceRoot": "src",
      "prefix": "app",
      "schematics": {},
      "root": "",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "karmaConfig": "karma.conf.js",
            "tsConfig": "tsconfig.spec.json",
            "polyfills": "src/polyfills.ts",
            "main": "src/test.ts",
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "build": {
          "configurations": {
            "production": {
              "buildOptimizer": true,
              "optimization": true,
              "sourceMap": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumError": "5mb",
                  "maximumWarning": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumError": "10kb",
                  "maximumWarning": "6kb"
                }
              ],
              "aot": true,
              "fileReplacements": [
                {
                  "with": "src/environments/environment.prod.ts",
                  "replace": "src/environments/environment.ts"
                }
              ],
              "extractCss": true,
              "namedChunks": false,
              "vendorChunk": false,
              "outputHashing": "all",
              "extractLicenses": true
            }
          },
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "outputPath": "dist/exampleClient",
            "aot": false,
            "tsConfig": "tsconfig.app.json",
            "index": "src/index.html",
            "polyfills": "src/polyfills.ts",
            "main": "src/main.ts",
            "styles": [
              "src/styles/styles.scss",
              "node_modules/swagger-ui/dist/swagger-ui.css"
            ],
            "scripts": []
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "exampleClient:build"
          }
        },
        "e2e": {
          "configurations": {
            "production": {
              "devServerTarget": "exampleClient:serve:production"
            }
          },
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "exampleClient:serve"
          }
        },
        "serve": {
          "configurations": {
            "production": {
              "browserTarget": "exampleClient:build:production"
            }
          },
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "exampleClient:build"
          }
        }
      },
      "projectType": "application"
    }
  },
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "defaultProject": "exampleClient",
  "version": 1,
  "newProjectRoot": "projects"
}